<?xml version="1.0" encoding="utf-8" ?>
<VoiceCommands xmlns="http://schemas.microsoft.com/voicecommands/1.2">
  <CommandSet xml:lang="en-us" Name="CheckersGameCommandSet_en-us">
    <CommandPrefix>Checkers</CommandPrefix>
    <Example>Start a game of checkers 2.0</Example>

    <Command Name="newGame">
      <Example>play a new game of checkers</Example>
      <ListenFor>[play] [a] new game [of] checkers</ListenFor>
      <Feedback>Trying to start a new game of checkers</Feedback>
      <Navigate Target="tbd MainPage.xaml" />
    </Command>

    <Command Name="Play">
      <Example>play a game of hotseat</Example>
      <ListenFor>
        [play] [a] game [of] hotseat</ListenFor>
        <Feedback>trying to start a game of single player hotseat</Feedback>
        <Navigate Target="tbd"/>
      </Command>

    
  </CommandSet>
		
</VoiceCommands>